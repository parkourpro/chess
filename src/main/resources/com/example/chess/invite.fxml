<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="62.0" prefWidth="315.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chess.InviteController">
   <children>
      <TextField fx:id="characternameTextField" layoutX="14.0" layoutY="19.0" promptText="Character name" />
      <Button fx:id="inviteButton" layoutX="187.0" layoutY="19.0" mnemonicParsing="false" onAction="#invite" text="Invite" />
      <Button fx:id="cancelButton" layoutX="253.0" layoutY="19.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
